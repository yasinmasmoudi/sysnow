        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {

	var m2m = new GlideRecord(current.getTableName());
m2m.addQuery("order", current.getValue("order"));
m2m.query();

var total = 0;

while (m2m.next()){
total = total + m2m.anzahl * m2m.product.price;
}

var order = new GlideRecord("x_879751_silivery_order");
if(order.get(current.order)){
order.setValue("order_price",total);
order.update();
}
})(current, previous);]]></script>
